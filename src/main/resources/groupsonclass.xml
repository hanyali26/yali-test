                <include name="stu"/>
            </run>
        </groups>

        <classes>
            <class name="com.course.parts.Groups.GroupsOnClass1"/>
            <class name="com.course.parts.Groups.GroupsOnClass2"/>
            <class name="com.course.parts.Groups.GroupsOnClass3"/>
        </classes>
    </test>

</suite>